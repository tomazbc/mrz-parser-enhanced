{
    "name": "tomazbc/mrz-parser-enhanced",
    "description": "Enhanced MRZ (Machine Readable Zones) parser with ISO 3166-1 alpha-2 country code support.",
    "keywords": [
        "tomazbc",
        "mrz-parser",
        "mrz-parser-enhanced",
        "iso-3166",
        "alpha-2",
        "country-codes"
    ],
    "homepage": "https://github.com/tomazbc/mrz-parser-enhanced",
    "license": "MIT",
    "authors": [
        {
            "name": "Md. Rakibul Islam",
            "email": "rakib1708@gmail.com",
            "role": "Original Developer"
        },
        {
            "name": "Tomaž Briški",
            "email": "tomaz@briski.org",
            "role": "Enhanced Fork Maintainer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Tomazbc\\MrzParserEnhanced\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tomazbc\\MrzParserEnhanced\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
